go TO src
 ./run_autoconfig
 mkdir build
 cd build/
 ../configure


sudo dnf install --allowerasing --skip-broken \
  https://download1.rpmfusion.org/free/fedora/rpmfusion-free-release-$(rpm -E %fedora).noarch.rpm \
  https://download1.rpmfusion.org/nonfree/fedora/rpmfusion-nonfree-release-$(rpm -E %fedora).noarch.rpm && \
sudo dnf install --allowerasing --skip-broken \
  nettle-devel wine-devel avahi-devel libdvbv5-devel ffmpeg-devel fuse-devel \
  freeglut-devel gtk+-devel libglade2-devel gtksourceview5-devel \
  libgnomecanvas-devel libgnome-devel libgnomeui-devel fftw-devel \
  mysql-devel unixODBC-devel freetds-devel postgresql-devel pcre-devel \
  sane-backends-devel sqlite-devel libsass-devel
